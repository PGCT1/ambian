
a(href='#', ng-click='linkClick("{{article.Link}}")', target='_blank')
	.sourceLogo(ng-style='{"background-image":"url({{article.sourceLogo}})"}')

	.texts
		h1.title {{article.Content.Title}}

		h2.sourceName {{article.Source}}
			img.verified(src='images/verified.png')

		p.text {{article.Content.Description}}
